registry=https://registry.npmjs.org/
strict-ssl=true
fetch-retries=5
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000

# Skip browser downloads for Puppeteer and Playwright to speed up builds
# Use npm config keys (lowercase) so postinstall scripts see them
puppeteer_skip_download=true
playwright_skip_browser_download=true
